{
  "name": "jquery-wheel",
  "type": "commonjs",
  "version": "0.2.4-pre",
  "description": "jQuery plugin creating a wheel event based on DOM3 wheel event",
  "homepage": "https://github.com/mgol/jquery-wheel",
  "author": {
    "name": "Michał Gołębiowski-Owczarek",
    "email": "m.goleb@gmail.com"
  },
  "keywords": [
    "jquery",
    "plugin",
    "ecosystem:jquery",
    "jquery-plugin",
    "browser",
    "browseronly"
  ],
  "main": "dist/jquery.wheel.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mgol/jquery-wheel.git"
  },
  "bugs": "https://github.com/mgol/jquery-wheel/issues",
  "license": "MIT",
  "files": [
    "dist/jquery.wheel.js",
    "dist/jquery.wheel.min.js",
    "dist/jquery.wheel.min.js.map",
    "LICENSE.txt"
  ],
  "devDependencies": {
    "eslint": "9.39.1",
    "eslint-config-mgol": "0.0.50",
